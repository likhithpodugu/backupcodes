#include <stdio.h>

int main(){
    int max = -999999 ;
    int min = 99999999 ;
    
    int n ;
    int lists[n] ;
    
    scanf("%d",&n) ;
    for(int i = 0 ;i<n ;i++){
        scanf("%d",&lists[i]) ;
    }
    
    for(int i = 0 ;i<n;i++){
        if(lists[i] > max){
            max = lists[i] ;
        }
        
        if(lists[i] < min){
            min = lists[i] ;
        }
    }
    
    
    
    printf("%d %d", min , max) ;
}
